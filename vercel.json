{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true,
      "preview": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "buildEnvironment": {
    "NEXT_PUBLIC_ENV": "${NEXT_PUBLIC_ENV}"
  }
}

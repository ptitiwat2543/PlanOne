{
  "name": "planone",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "next dev --turbopack",
    "build": "next build",
    "build:preview": "dotenv -e .env.preview -- next build",
    "build:production": "dotenv -e .env.production -- next build",
    "start": "next start",
    "start:preview": "dotenv -e .env.preview -- next start",
    "start:production": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint && npm run format:check && npm run type-check",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "dotenv -e .env.development -- tsx src/db/migrate.ts",
    "db:migrate:preview": "dotenv -e .env.preview -- tsx src/db/migrate.ts",
    "db:migrate:production": "dotenv -e .env.production -- tsx src/db/migrate.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptitiwat2543/PlanOne.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ptitiwat2543/PlanOne/issues"
  },
  "homepage": "https://github.com/ptitiwat2543/PlanOne#readme",
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-toast": "^1.2.11",
    "@reduxjs/toolkit": "^2.7.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.75.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "framer-motion": "^12.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.507.0",
    "next": "^15.3.1",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-redux": "^9.2.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.3.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
